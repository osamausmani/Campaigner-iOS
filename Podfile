  pod 'GoogleSignInSwiftSupport'
  pod 'Firebase/Auth'
  pod 'Firebase/Analytics'
  

  pod 'FBSDKCoreKit'
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'
  pod 'FBSDKShareKit'
end
